{
  "presets": [
    "react-app",
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ],
    "transform-decorators-legacy"
  ],
  "env": {
      "development": {
      "plugins": [["react-transform", {
         "transforms": [{
           "transform": "react-transform-hmr",
           
           "imports": ["react"],
           
           "locals": ["module"]
         }]
       }]]
      }
  },
}
